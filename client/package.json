{
  "name": "grocery_comparison",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "tsc && vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "npx cypress run",
    "test:unit": "vitest",
    "test:ci": "jest --config ./jest.config.js --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false | perl -pe 's/\\e\\[?.*?[\\@-~]//g'",
    "start": "ionic serve",
    "start:server": "cd ../server && nodemon",
    "serve:android": "concurrently \"npm run start:server\" \"npm run ionic:android\"",
    "serve:ios": "concurrently \"npm run start:server\" \"npm run ionic:ios\"",
    "ionic:android": "ionic capacitor run android --livereload --external --public-host=192.168.x.x",
    "ionic:ios": "ionic capacitor run ios",
    "test": "./node_modules/.bin/jest",
    "commitlint": "./node_modules/.bin/commitlint"
  },
  "dependencies": {
    "@capacitor/android": "6.2.0",
    "@capacitor/app": "6.0.2",
    "@capacitor/core": "6.2.0",
    "@capacitor/haptics": "6.0.2",
    "@capacitor/ios": "6.2.0",
    "@capacitor/keyboard": "6.0.3",
    "@capacitor/status-bar": "6.0.2",
    "@ionic/react": "^8.0.0",
    "@ionic/react-router": "^8.0.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.7.9",
    "chart.js": "^4.4.7",
    "glob": "^11.0.0",
    "history": "^5.3.0",
    "ionicons": "^7.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@capacitor/cli": "6.2.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^14.0.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "jsdom": "^22.1.0",
    "terser": "^5.4.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "5.2.14",
    "vitest": "^0.34.6"
  },
  "description": "A grocery comparison app."
}
