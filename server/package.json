    "start": "ts-node src/server.ts",
    "test": "./node_modules/.bin/jest | perl -pe 's/\\e\\[?.*?[\\@-~]//g'",
    "tests": "npx jest",
    "dev": "nodemon --legacy-watch src/server.ts",
    "scrape": "tsx src/web-scraping/main.ts",
    "test:ci": "jest --config ./jest.config.js --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false | perl -pe 's/\\e\\[?.*?[\\@-~]//g'",
    "db:generate": "npx drizzle-kit generate --config ./drizzle.config.ts",
    "db:pull": "npx drizzle-kit pull --config ./drizzle.config.ts",
    "db:push": "npx drizzle-kit push --force --config ./drizzle.config.ts",
    "db:show": "npx drizzle-kit studio --config ./drizzle-dev.config.ts",
    "db:reset": "npx ts-node src/server.ts reset",
    "db:seed": "npx ts-node src/server.ts seed",
    "db:drop": "npx ts-node src/server-db-maintenance.ts drop",
